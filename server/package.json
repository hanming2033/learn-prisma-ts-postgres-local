{
  "name": "prismaboilerplate",
  "scripts": {
    "start": "dotenv -- nodemon -e ts,graphql -x ts-node src/index.ts",
    "dev": "npm-run-all --parallel start playground",
    "debug": "dotenv -- nodemon -e ts,graphql -x ts-node --inspect src/index.ts",
    "playground": "graphql playground",
    "build": "rimraf dist && tsc",
    "gentypes":
      "gql-gen --schema http://localhost:4000 --template graphql-codegen-typescript-template --out ./src/generated/server-types.d.ts"
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
    "graphql-yoga": "1.14.6",
    "jsonwebtoken": "8.2.1",
    "prisma-binding": "2.0.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.1",
    "@types/jsonwebtoken": "^7.2.7",
    "dotenv-cli": "1.4.0",
    "gql2ts": "^1.8.0",
    "graphql": "^0.13.2",
    "graphql-cli": "2.16.0",
    "graphql-code-generator": "^0.9.1",
    "graphql-codegen-typescript-template": "^0.9.1",
    "nodemon": "1.17.5",
    "npm-run-all": "4.1.3",
    "prettier": "^1.13.4",
    "prisma": "1.8.3",
    "rimraf": "2.6.2",
    "ts-node": "6.0.5",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "typescript": "2.8.3"
  }
}
